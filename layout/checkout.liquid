              <!-- Zeta Digital - Pre-order Product Realease Date -->
              <script>
                var datePattern = /[0-9]{2,}-*[a-zA-Z]{3,}-*[0-9]{2,}/i;
                var monthPattern = /[a-zA-Z]{3,}-*[0-9]{2,}/i;
                var quarterPattern = /[a-zA-Z0-9]{2,}-*[0-9]{2,}/i;
              </script>
              {% for item in checkout.line_items %}
                <script>
                  var productId = `{{ item.product.id }}`;
                  var productParentElement = document.querySelector(`[data-product-id = '${productId}']`);
                  // Add Product Qty - Start
                  var productPriceElement = productParentElement.getElementsByClassName("product__price")[0];
                  var quantity = `{{ item.quantity }}`;
                  productPriceElement.insertAdjacentHTML('beforeend', `<br><span style="color: #323232; font-weight: 500;">Qty: ${quantity}</span>`);
                  // Add Product Qty - End
                  {% if item.product.description contains "Release Date:" %}
                      var description = `{{ item.product.description | escape }}`;
                      description = description.substr(description.search("Release Date:"), 100);
                      var releaseDate = description.match(datePattern);
                      var productDescVariantElement = productParentElement.getElementsByClassName("product__description__variant")[0];
                      if (releaseDate != null) {
                          productDescVariantElement.insertAdjacentHTML('beforeend', `<span style="color:red;">Release Date: ${releaseDate}</span>`);
                      } else {
                          var releaseMonth = description.match(monthPattern);
                          if (releaseMonth != null) {
                               productDescVariantElement.insertAdjacentHTML('beforeend', `<span style="color:red;">Release Date: ${releaseMonth}</span>`);
                          } else {
                              var releaseQuarter = description.match(quarterPattern);
                              if (releaseQuarter != null) {
                                productDescVariantElement.insertAdjacentHTML('beforeend', `<span style="color:red;">Release Date: ${releaseQuarter}</span>`);
                              }
                          }
                      }
                  {% endif %}
                </script>
