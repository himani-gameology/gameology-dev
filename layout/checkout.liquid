        function getDayOfWeek(dateString) {
            let date;

            if (dateString.toLowerCase() === 'today') {
              date = new Date();
              date.setDate(date.getDate());
            } else {
              const cleanedDate = dateString.replace(/\b(\d+)(st|nd|rd|th)\b/g, '$1');

              date = new Date(cleanedDate);
              date.setFullYear(new Date().getFullYear());
            }

            const days = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'];
            const months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
            const monthName = months[date.getMonth()];


            return formatDateForQueryString(`${days[date.getDay()]}, ${date.getDate()} ${monthName} ${date.getFullYear()}`);
        }

        function formatDateForQueryString(dateString, revert = false) {

          if(!revert) {
            const dateParts = dateString.split(', ');
            const dayMonthYear = dateParts[1].split(' ');

            const day = dateParts[0];
            const date = dayMonthYear[0];
            const month = dayMonthYear[1];
            const year = dayMonthYear[2];

            const formattedDate = `${day}-${date}-${month}-${year}`;

            return formattedDate;
          } else {
            const dateParts = dateString.split('-');
            const day = dateParts[0];
            const date = parseInt(dateParts[1]);
            const month = dateParts[2];
            const year = dateParts[3];

            const reversedDate = `${day}, ${date} ${month} ${year}`;

            return reversedDate;
          }
        }

        function getQueryParam(name) {
