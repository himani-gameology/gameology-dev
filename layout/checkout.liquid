                <script>
                  const changePaymentPosition = () => {
                      const paypalBtn = document.querySelector('iframe[title*="PayPal"]');
                      if(paypalBtn) {
                          const customBtn = document.getElementById('express-checkout-methods');
                          if(customBtn) {
                            paypalBtn.closest('.dynamic-checkout__buttons').classList.add('hide-test_157');
                            paypalBtn.closest('.dynamic-checkout__buttons').insertAdjacentElement('afterend', customBtn);
                            paypalBtn.closest('ul').setAttribute("style", "display: flex !important; flex-direction: column-reverse !important;");
                            customBtn.style.display = 'block';
                          }

                          document.querySelectorAll('.js-block-change-position')?.forEach(el => {
                              const element = el.cloneNode(true);
                              el.classList.remove('js-block-change-position');
                              el.classList.add('hide-test_157');

                              element.classList.remove('js-block-change-position');
                              element.classList.add('show-test_157', 'hidden_task');
                              const div = document.createElement('div');
                              div.append(element);
                              div.classList.add('block-wrapper');
                              document.querySelector('.express-checkout-wrapper').insertAdjacentElement('afterbegin', div);
                          })
                      } else if(Shopify.Checkout.step == 'contact_information') {
                          setTimeout(() => changePaymentPosition(), 1000);
                      } else {
                          document.getElementById('express-checkout-methods')?.remove();
                      }
                  }
                  changePaymentPosition();
                </script>
