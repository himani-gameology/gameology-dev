              <!-- Conversionry - Timer Countdown (30/11/2021) -->
              <script>
                class cartCountDown extends HTMLElement {
                    constructor() {
                        super();
                        let timer = this.dataset.countdown || 10;
                        this.timer = timer * 60 * 1000;
                    }

                    connectedCallback() {
                        this.createObserver();
                    }

                    getDeadline() {
                        let deadline = +localStorage.getItem('cartCountDown') || (new Date().getTime() + this.timer);
                        const remain = deadline - (new Date().getTime())

                        if(remain < 0) {
                            deadline = (new Date().getTime() + this.timer);
                        }
                        return deadline;
                    }

                    createObserver() {
                        let observer = new IntersectionObserver((entries, observer) => {
                            if(entries[0].intersectionRect.x > 0) {
                                this.deadline = this.getDeadline();
                                requestAnimationFrame(this.countDown.bind(this));
                                observer.disconnect();
                            }
                        });

                        observer.observe(this);
                    }

                    pad(value) {
                        return ('0' + Math.floor(value)).slice(-2);
                    }

                    countDown() {
                        this.currentTime = new Date().getTime();
                        const remain = this.deadline - this.currentTime;
                        const seconds = this.pad((remain / 1000) % 60);
                        const minutes = this.pad((remain / (60 * 1000)) % 60);

                        this.innerHTML = `${minutes}:${seconds}`;

                        if(remain >= 1000) {
                            localStorage.setItem('cartCountDown', this.deadline)
                            requestAnimationFrame(this.countDown.bind(this));
                        }
                    }
                }
                customElements.define('cart-countdown', cartCountDown);
              </script>
