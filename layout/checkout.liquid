                <!-- Conversionry - Save Amount -->
                {%- assign quantity = item.quantity -%}
                {%- assign price = item.price -%}
                {%- assign onSale = false -%}

                {%- if item.variant.compare_at_price > item.price -%}
                  {%- assign onSale = true -%}
                  {%- assign price_compare = item.variant.compare_at_price -%}
                {%- endif -%}

                {% if onSale %}
                  {% assign item_compare = price_compare | times: quantity %}
                {% else %}
                  {% assign item_compare = price | times: quantity %}
                {% endif %}
                {% assign cart_compare = cart_compare | plus: item_compare %}
