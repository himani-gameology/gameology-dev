          let attempts = 0, maxAttempts = 50;
          function dynamicShippingEta() {
            const Holidays = window.Holidays.default;
            let hd = new Holidays();
            const csv = `{{ settings.checkout_criteria_csv_standard }}`;
            const csvLight = `{{ settings.checkout_criteria_csv_standard_light }}`;
            const csvExpress = `{{ settings.checkout_criteria_csv_express }}`;
            const shippingOptions = document.querySelectorAll('.radio-wrapper');

            if (shippingOptions.length === 0 && attempts < maxAttempts) {
              setTimeout(dynamicShippingEta, 100);
              attempts++;
              return;
            }

            const address = document.querySelector('.address.address--tight').innerText;
            const addressArr = address.split(',');
            const postcode = addressArr[addressArr.length - 2].trim().split(' ').slice(-1)[0];

            const formatCSV = (csv) => {
              if(!csv) return [];
              return csv.slice(1).map(item => {
                let firstIndex = 0;
                let sliceN = 1;
                for (let i = 0; i < item[0].length; i++) {
                    if (parseInt(item[0][i]) && item[0][i - 1].indexOf(' ') >= 0 || item[0][i] === "0" && item[0][i - 1].indexOf(' ') >= 0) {
                        firstIndex = i;
                        break;
                    }
                }
                let state = item[0].slice(0, firstIndex -1);
                let postcodes = item[0].slice(firstIndex).split(/[ ,]+/);
                postcodes = postcodes.filter(postcode => postcode.length > 0);
                let [dates] = postcodes.slice(postcodes.length - 1);
                if (dates.includes('days')) {
                  [dates] = postcodes.slice(postcodes.length - 3);
                  sliceN = 3
                }
                postcodes.length = postcodes.length - sliceN;
                postcodes = postcodes.join(',');

                return { state, postcodes, dates };
              });
            }

            const csvStringToArray = strData => {
              if(!strData) return [];
                const objPattern = new RegExp(("(\\,|\\r?\\n|\\r|^)(?:\"([^\"]*(?:\"\"[^\"]*)*)\"|([^\\,\\r\\n]*))"),"gi");
                let arrMatches = null, arrData = [[]];
                while (arrMatches = objPattern.exec(strData)){
                    if (arrMatches[1].length && arrMatches[1] !== ",")arrData.push([]);
                    arrData[arrData.length - 1].push(arrMatches[2] ?
                        arrMatches[2].replace(new RegExp( "\"\"", "g" ), "\"") :
                        arrMatches[3]);

                    arrData[arrData.length - 1][arrData[arrData.length - 1].length - 1] = arrData[arrData.length - 1][arrData[arrData.length - 1].length - 1].replace(/\s+/g, ' ').trim();

                    if(arrData[arrData.length - 1].length > 1) {
                        arrData[arrData.length - 1] = [arrData[arrData.length - 1].join(' ')];
                    }
                }
                return arrData;
            }

            const getStateShortName = (state) => {
              let shortName;
              if (state.includes('Victoria')) {
                shortName = 'VIC';
              } else if (state.includes('Northern Territory')) {
                shortName = 'NT';
              } else if (state.includes('Western Australia')) {
                shortName = 'WA';
              } else if (state.includes('South Australia')) {
                shortName = 'SA';
              } else if (state.includes('Queensland')) {
                shortName = 'QLD';
              } else if (state.includes('Tasmania')) {
                shortName = 'TAS';
              } else if (state.includes('New South Wales')) {
                shortName = 'NSW';
              } else if (state.includes('Australian Capital Territory')) {
                shortName = 'ACT';
              }

              return shortName;
            }


            const csvArr = csvStringToArray(csv);
            const csvLightArr = csvStringToArray(csvLight);
            const csvExpressArr = csvStringToArray(csvExpress);

            const formattedCsvData = formatCSV(csvArr);
            const formattedCsvLightData = formatCSV(csvLightArr);
            const formattedCsvExpressData = formatCSV(csvExpressArr);


            const match = [];
            const matchLight = [];
            const matchExpress = [];

            if(formattedCsvData) {
              formattedCsvData.forEach(({state, postcodes, dates }) => {
                if (match.length) {
                  return false;
                }


                postcodes.split(',').forEach(postCodeRange => {
                  let min,max = 0;

                  if (postCodeRange.includes('-')) {
                    const range = postCodeRange.split('-');
                    min = range[0];
                    max = range[1];

                  } else {
                    min = max = postCodeRange;
                  }

                  if (postcode >= min && postcode <= max) {
                    match.push({ state, postcodes, dates });
                    return false;
                  }
                });
              });
            }

            if(formattedCsvLightData) {
              formattedCsvLightData.forEach(({state, postcodes, dates }) => {
                if (matchLight.length) {
                  return false;
                }

                postcodes.split(',').forEach(postCodeRange => {
                  let min,max = 0;

                  if (postCodeRange.includes('-')) {
                    const range = postCodeRange.split('-');
                    min = range[0];
                    max = range[1];

                  } else {
                    min = max = postCodeRange;
                  }

                  if (postcode >= min && postcode <= max) {
                    matchLight.push({ state, postcodes, dates });
                    return false;
                  }
                });
              });
            }

            if(formattedCsvExpressData) {
              formattedCsvExpressData.forEach(({state, postcodes, dates }) => {
                if (matchExpress.length) {
                  return false;
                }

                postcodes.split(',').forEach(postCodeRange => {
                  let min,max = 0;

                  if (postCodeRange.includes('-')) {
                    const range = postCodeRange.split('-');
                    min = range[0];
                    max = range[1];

                  } else {
                    min = max = postCodeRange;
                  }

                  if (postcode >= min && postcode <= max) {
                    matchExpress.push({ state, postcodes, dates });
                    return false;
                  }
                });
              })
            }


            if(match.length) {
              const { state, dates } = match[0];

              const year = new Date().getFullYear();

              const standardDate = new Date();
              const nthNumber = (number) => {
                return number > 0
                  ? ["th", "st", "nd", "rd"][
                      (number > 3 && number < 21) || number % 10 > 3 ? 0 : number % 10
                    ]
                  : "";
              };



              if (!dates.includes('-')) {
                const shortName = getStateShortName(state);
                hd.init('AU', shortName);
                const statesHolidays = hd.getHolidays(year);

                if (standardDate.getDay() === 0) {
                  standardDate.setDate(standardDate.getDate() + 1);
                } else if (standardDate.getDay() === 6) {
                  standardDate.setDate(standardDate.getDate() + 2);
                } else if (standardDate.getDay() === 5) {
                  standardDate.setDate(standardDate.getDate() + 3);
                }

                standardDate.setDate(standardDate.getDate() + parseInt(dates));
                

                statesHolidays.forEach(holiday => {
                  const holidayDate = new Date(holiday.date);
                  if (standardDate.toDateString() === holidayDate.toDateString() && holiday.type === 'public') {
                    standardDate.setDate(standardDate.getDate() + 1);
                  }
                });



        




                const standardDay = standardDate.getDate();
                // const expressMonth = expressDate.toLocaleString("default", { month: "long" });;
                const standardMonth = standardDate.toLocaleString("default", { month: "long" });
                const fastestIcon = `<span class="icon fastest" style="background-image: url('data:image/svg+xml;base64,')"></span>`;
                let hasSameDayShipping = false;
                shippingOptions.forEach(function(shippingOption){
                  if (jQuery(shippingOption).data('shipping-method') && jQuery(shippingOption).data('shipping-method').includes('shopify-Standard%20shipping-10.00')){
                    if(shippingOption.children[1].children[0].querySelector('.shipping-date')) {
                      shippingOption.children[1].children[0].querySelector('.shipping-date').remove();
                    }
                    

                    shippingOption.children[1].children[0].innerHTML += `<span class="shipping-date">Estimated by ${standardDay}${nthNumber(standardDay)} ${standardMonth}</span><span class="icon cheapest" data-shipping="standard" style="background-image: url('data:image/svg+xml;base64,')"></span>`;
                  }
                  else if(shippingOption.getAttribute('data-shipping-method')?.includes('Shipping-tonite_delivery')) {
                    if(shippingOption.children[1].children[0].querySelector('.shipping-date')) {
                      shippingOption.children[1].children[0].querySelector('.shipping-date').remove();
                    }

                    hasSameDayShipping = true;
                    if(shippingOption.children[1].children[0].innerText.includes('Tomorrow Night Delivery')) {
                      shippingOption.children[1].children[0].innerText = 'Tomorrow Night Delivery';
                      shippingOption.children[1].children[0].innerHTML += `<span class="shipping-date">No PO Boxes, Business Addresses, Parcel Lockers, and Parcel Collect. Arrives 6pm-9pm tomorrow night </span>${fastestIcon}`;

                    } else if(shippingOption.children[1].children[0].innerText.includes('Next Business Day Delivery')) {
                      shippingOption.children[1].children[0].innerText = 'Next Business Day Delivery';
                      shippingOption.children[1].children[0].innerHTML += `<span class="shipping-date">No PO Boxes, Business Addresses, Parcel Lockers, and Parcel Collect. Arrives 6pm-9pm next business day </span>${fastestIcon}`;

                    } else {
                      shippingOption.children[1].children[0].innerText = 'Same-Day Delivery';
                      shippingOption.children[1].children[0].innerHTML += `<span class="shipping-date">No PO Boxes, Business Addresses, Parcel Lockers, and Parcel Collect. Arrives 6pm-9pm same business day </span>${fastestIcon}`;
                    }

                    jQuery(shippingOption.children[1].children[0]).addClass('date-edited');
                  }
                });
