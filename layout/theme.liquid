<script type="text/javascript">
  // Redirect account page to product page
  if (typeof(Storage) !== 'undefined') {
    var checkBody = document.querySelector('body');
    if(hasClass(checkBody, 'js-return-product') == true || hasClass(checkBody, 'template-index') == true) {
      var checkProduct1 = localStorage.getItem('ProductLogin');
      var checkProduct2 = localStorage.getItem('ProductCreate');
      if (checkProduct1 != null) {
        window.location.href = checkProduct1;
      } else if (checkProduct2 != null) {
        window.location.href = checkProduct2;
      }
    }

    function hasClass(element, cls) {
      return (' ' + element.className + ' ').indexOf(' ' + cls + ' ') > -1;
    }
  }
</script>

