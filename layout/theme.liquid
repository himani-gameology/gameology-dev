function openReplacement() {
  this.addEventListener("load", function () {
    if (
      [
        "/cart/add.js",
        "/cart/update.js",
        "/cart/change.js",
        "/cart/clear.js",
      ].includes(this._url)
    ) {
        var cartOrItem = JSON.parse(this.response);
        if (cartOrItem != null && ((cartOrItem.item_count != null && cartOrItem.item_count > 0 ) || (cartOrItem.quantity != null && cartOrItem.quantity > 0))){
        	jQuery("#favicon").attr("href","https://cdn.shopify.com/s/files/1/0921/7330/files/favicon_32x32_red_dot.png?v=1656394157");
