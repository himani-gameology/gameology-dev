		<title>{title}</title>
        <style>
			.header-top .header-top-menu li a.home_menu {
				color:#000;
			}
			.header-content .header-right a.message{
				color:#000;
			}
			.header-content , .header-top{
				background-color: #fff;
			}
			.header-right .logo-content{
				width: 100px;
			}
			.header-bottom{
				background: #000;
    			height: 5px;
			}
			.product-shop-container{
				margin-top: 25px;
			}
			.product-shop-container .trus-meker{
				padding: 25px 0;
			}
			.product-shop-container.info-section .trus-meker{
				border: none;
    			border-bottom: none;	
			}
			.product-shop-container.info-section .info-list .image-span{
				width: 80px;
			}
			.product-shop-container.info-section .trus-meker ul.info-box li{
				margin:10px 0;
			}
			.wrappage .footer{
				background: #fff;
			}
			.footer-bottom-content {
				background: #fff;
			}
			.footer-right .box-footer:last-child li a ,.footer-right .box-footer h4 , .footer-right .box-footer .footer__section ,.footer-right .box-footer .footer__section span a , .footer-right .box-footer .footer__section #footer-find-us-table ,.footer-right .box-footer .footer__section .footer__section-head h5{
