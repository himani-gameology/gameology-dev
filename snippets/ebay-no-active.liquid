						         <div class="grid-item"></div>
						         <div class="grid-item"></div>
						         <div class="grid-item"></div>
						         <div class="grid-item"></div>
						         <div class="grid-item"></div>
						         {for AllEnabledImages step=12}
									<div class="grid-content">
										{for AllEnabledImages} <input name="slide_switch"
											id="id{position}" value="{AllEnabledImages.url}" type="radio">
											<label for="id{position}"> <span><img
													src="{AllEnabledImages.url}&width=200&height=200"
													alt="{title}" /></span>
										</label>
											<div class="img-details">
