function estimatedTime(dayPlus, cutoffHour) {
	let estDay = new Date();
	let now = new Date();
	let dayNow = now.getDay();
	let hourNow = now.getHours();
	let z = 0;

	if (dayPlus) {
		while (z < dayPlus) {
			let newDay = estDay.getDay();
			estDay.setDate(estDay.getDate() + 1);
			newDay = estDay.getDay();

			if (newDay == code.saturday) {
				estDay.setDate(estDay.getDate() + 1);
				newDay = estDay.getDay();
			}
			if (newDay == code.sunday) {
				estDay.setDate(estDay.getDate() + 1);
				newDay = estDay.getDay();
			}
			while (isPublicHoliday(estDay)) {
				estDay.setDate(estDay.getDate() + 1);
				newDay = estDay.getDay();
			}
			z++;
		}
	} else {
		let deliveryDay;
		var isPublicHolidayToday = isPublicHoliday(estDay);
		while (isPublicHoliday(estDay)) {
			estDay.setDate(estDay.getDate() + 1);
		}

		if (estDay.getDay() == 0 || estDay.getDay() == 6 || (estDay.getDay() == 5 && estDay.getHours() >= cutoffHour) || isPublicHolidayToday) {
			estDay.getDay() == 0 && estDay.setDate(estDay.getDate() + 1);
			estDay.getDay() == 6 && estDay.setDate(estDay.getDate() + 2);
			estDay.getDay() == 5 && hourNow >= cutoffHour && estDay.setDate(estDay.getDate() + 3);
			deliveryDay = "Next Business Day";
		} else {
			if (estDay.getHours() >= cutoffHour) {
				estDay.setDate(estDay.getDate() + 1);
				deliveryDay = "Tomorrow";
			} else if (estDay.getHours() >= 0) {
				deliveryDay = "Tonight";
			}
		}

		while (isPublicHoliday(estDay)) {
			estDay.setDate(estDay.getDate() + 1);
			estDay.getDay() == 0 && estDay.setDate(estDay.getDate() + 1);
			estDay.getDay() == 6 && estDay.setDate(estDay.getDate() + 2);
			estDay.getDay() == 5 && hourNow >= cutoffHour && estDay.setDate(estDay.getDate() + 3);
			deliveryDay = "Next Business Day";
		}


		estDay.setHours(cutoffHour);
		estDay.setMinutes(0);
		estDay.setSeconds(0);


		return {
			deliveryDay,
			deliveryDate: estDay
		};
	};

	estDay.setHours(cutoffHour);
	estDay.setMinutes(0);
	estDay.setSeconds(0);

	return estDay;
}

