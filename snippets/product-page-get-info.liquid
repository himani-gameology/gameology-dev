<script>
  jQuery(document).ready(function() {
    if (jQuery("#preorder-release-date").html().trim() != '') {
      var releaseDate = jQuery("#preorder-release-date").html().trim();
      
      function dateToQuarter(dateStr) {
        const quarterMap = {
          'January': 'Q1', 'February': 'Q1', 'March': 'Q1',
          'April': 'Q2', 'May': 'Q2', 'June': 'Q2',
          'July': 'Q3', 'August': 'Q3', 'September': 'Q3',
          'October': 'Q4', 'November': 'Q4', 'December': 'Q4'
        };
        const dateParts = dateStr.split('-');
        if (dateParts.length === 3) {
          const day = dateParts[0].trim();
          const month = dateParts[1].trim();
          const year = dateParts[2].trim();
          const quarter = quarterMap[month];
          if (quarter) {
            return `${quarter}-${year}`;
          } else {
            console.error("Month parsing error, check month input: " + month);
            return dateStr;
          }
        }
        return dateStr;
      }

      releaseDate = dateToQuarter(releaseDate);

      jQuery("#preorder-date-block").html(`<span style="display: flex;justify-content: center;padding-top:2px;">` + `Release ` + releaseDate + `</span>`);
    } else {
      jQuery("#preorder-date-block").hide();
    }
  });
</script>

