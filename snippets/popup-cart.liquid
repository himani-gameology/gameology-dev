<script>
  var signInLink = document.getElementById('sign-in-link');
  var modal = document.getElementById('sign-in-modal');
  var closeModalElements = document.querySelectorAll('.close-modal');
  var showRegisterForm = document.getElementById('show-register-form');
  var showSignInForm = document.getElementById('show-sign-in-form');
  var signInForm = document.getElementById('sign-in-form');
  var registerForm = document.getElementById('register-form');

  if (signInLink) {
    signInLink.addEventListener('click', function(event) {
      event.preventDefault();
      modal.style.display = 'block';
    });
  }

  closeModalElements.forEach(function(element) {
    element.addEventListener('click', function() {
      modal.style.display = 'none';
    });
  });

  window.addEventListener('click', function(event) {
    if (event.target === modal) {
      modal.style.display = 'none';
    }
  });

  if (showRegisterForm) {
    showRegisterForm.addEventListener('click', function(event) {
      event.preventDefault();
      signInForm.style.display = 'none';
      registerForm.style.display = 'block';
    });
  }

  if (showSignInForm) {
    showSignInForm.addEventListener('click', function(event) {
      event.preventDefault();
      registerForm.style.display = 'none';
      signInForm.style.display = 'block';
    });
  }
</script>
                            
