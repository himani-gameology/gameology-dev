<!-- Sign-In Modal Structure -->
<div id="sign-in-modal" class="modal-wrapper" style="display: none;">
  <div class="modal-content">
    <div class="popup-account__head d-flex align-items-center mb-10">
      {% include 'logo' svg: settings.logo_mobile_svg_file image: settings.logo_mobile_image width: settings.logo_mobile_width %}
      <i class="popup-account__close ml-auto cursor-pointer close-modal">
        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
          <path d="M18 6L6 18M6 6L18 18" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
        </svg>
      </i>
    </div>
            
    <!-- Sign-In Form -->
    <div id="sign-in-form">
      <h3>Sign In</h3>
      {% form 'customer_login', id: 'popup_customer_login', class: 'form-group--type-inline mb-15' %}
        <div>
          <label for="PrimageopupCustomerEmail" class="label-required">{{ 'general.popups.account.login.email.title' | t }}</label>
          <input type="email" name="customer[email]" id="PopupCustomerEmail" class="{% if form.errors contains 'email' %}input-error{% endif %}" placeholder="{{ 'general.popups.account.login.email.placeholder' | t }}" spellcheck="false" autocomplete="off" autocapitalize="off" required="required">
        </div>
        {%- if form.password_needed -%}
        <div>
          <label for="PopupCustomerPassword" class="label-required">{{ 'general.popups.account.login.password.title' | t }}</label>
          <input type="password" name="customer[password]" id="PopupCustomerPassword" class="{% if form.errors contains 'password' %}input-error{% endif %}" placeholder="{{ 'general.popups.account.login.password.placeholder' | t }}" required="required">
        </div>
        {%- endif -%}
        {% include 'form-get-message' %}
        <input type="submit" class="popup-account__btn-login btn btn--full mb-20" value="Login">
        <div class="mb-15">
          <a href="#" id="show-register-form" class="text-center"><u>Sign Up</u></a>
        </div>
      {% endform %}
    </div>

    <!-- Register Form -->
    <div id="register-form" style="display: none;">
      <h3>Sign Up</h3>
      {% form 'create_customer', return_to: routes.account_register_url %}
        <div>
          <label for="FirstName" class="label-required">{{ 'customer.register.first_name_title' | t }}</label>
          <input type="text" name="customer[first_name]" id="FirstName" placeholder="Enter your first name" required>
        </div>
        <div>
          <label for="LastName" class="label-required">{{ 'customer.register.last_name_title' | t }}</label>
          <input type="text" name="customer[last_name]" id="LastName" placeholder="Enter your last name" required>
        </div>
        <div>
          <label for="Email" class="label-required">{{ 'customer.register.email_title' | t }}</label>
          <input type="email" name="customer[email]" id="Email" placeholder="Enter your email" required>
        </div>
        <div>
          <label for="CreatePassword" class="label-required">{{ 'customer.register.password_title' | t }}</label>
          <input type="password" name="customer[password]" id="CreatePassword" placeholder="Enter your password" required>
        </div>
        {% include 'form-get-message' %}
        <input type="submit" value="Sign Up" class="popup-account__btn-register btn btn--full btn--secondary">
        <p class="text-center">
          <a href="#" id="show-sign-in-form" class="text-center"><u>Login</u></a>
        </p>
      {% endform %}
    </div>
            
  </div>
</div>

