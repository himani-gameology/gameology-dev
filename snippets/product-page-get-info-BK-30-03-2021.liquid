                <input type="hidden" name="variant_id" value="{{ current_variant.id }}" data-js-product-variant-input>
                {%- if show_options and show_custom_options -%}
                    {%- unless product.has_only_default_variant -%}
                        <div class="product-page-info__options mb-15">
                            {%- unless is_quick_view -%}
                                {%- assign trigger_id = 'footbar' -%}
                            {%- endunless -%}
                            {% include 'product-get-options' with options_type: 2 centered: true options_show_title: true change_history: change_history %}
                        </div>
                    {%- endunless -%}
                {%- endif -%}
                {%- if show_button_add_to_cart or show_options -%}
                    <div class="product-page-info__variants mb-15{% if show_options == false or show_custom_options %} d-none{% endif %}">
                        {% render 'product-get-variants' with product: product show_options: show_options show_custom_options: show_custom_options %}
                    </div>
                {%- endif -%}
                
                {%- if show_quantity and settings.product_info_button_layout != '3' -%}
                    <div class="product-page-info__field product-page-info__quantity product-page-info__section--max-width mx-lg-auto mb-20">
                        {%- unless is_quick_view -%}
                            {%- assign quantity_connect = 'footbar' -%}
                        {%- endunless -%}
                        {% render 'product-get-quantity' with quantity_show_title: true quantity_connect: quantity_connect %}
                    </div>
                {%- endif -%}
                {%- if show_notes -%}
                    <div class="product-page-info__field product-page-info__notes product-page-info__section--max-width mx-lg-auto mb-20">
                        <label>{{ 'products.product.notes_label' | t }}</label>
                        <textarea name="" placeholder="{{ 'products.product.notes_placeholder' | t }}" class="mb-0"></textarea>
                    </div>
                {%- endif -%}
                {% include 'product-page-get-buttons' %}
                {%- if settings.app_klarna_enable -%}
                    <div class="klarna-message-info2"></div>
                {%- endif -%}
                {{ free_shipping_html }}
                {{ payments_html }}
                {%- if show_social_share_buttons -%}
                    <div class="product-page-info__share product-page-info__field mt-20 mb-10">
                        <label class="mb-6">{{ 'products.product.share' | t }}</label>
                        {% render 'social-share' share_type: social_share_buttons_type share_title: title share_permalink: url share_image: product.featured_media %}
                    </div>
                {%- endif -%}
                {%- if full_details -%}
                    <div class="product-page-info__full-details d-flex justify-content-center justify-content-lg-start mt-15">
                        <a href="{{ url }}" class="btn-link h6">{{ 'products.product.button_full_details' | t }}</a>
                    </div>
                {%- endif -%}
            {% endform %}
        </div>
        {{ text_countdown_html }}
    </div>
{%- endcase -%}

<!-- Zeta Digital - Large Item Shipping Calculator Start -->
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
{{ 'au_suburb_postcode.js' | asset_url | script_tag }} 
<script>
  jQuery('#postcode-check').autocomplete({
    source: ausPostcode,
    minLength: 3
  });
  jQuery('#get-delivery-options-btn').on('click', function(){
  	if(jQuery('#postcode-check').val() == null || jQuery('#postcode-check').val() == ''){
      alert("Please input your postcode");
      return false;
    } 
    
    var selectedSuburb = jQuery('#postcode-check').val().split(',');
    var pweight = parseInt(jQuery('#pweight').val()) * 0.001;
    jQuery.ajax({
      dataType: "json",
      type: "GET",
      async: false,
      url: "https://api.gameology.com.au/api/shipping/quote/get?weight="+ pweight +"&length=11&height=14&width=13&suburb=" + selectedSuburb[0] + "&postcode=" + selectedSuburb[1].trim(),
      success: function(response)
      {
		  var outputText = '';
          jQuery.each(response.data.availableQuotes, function(index, itemData) {
            if(itemData.amount > 0){
 				  
                var divHtml = '<li>'+ itemData.serviceProvider + ' ' + itemData.displayName + ' - $'+itemData.amount+'</li>';
   				outputText += divHtml;
            } 
          });
          jQuery('.shipping_result ul').html(outputText);
       	 
      },
      error: function(){
      }
    });
  });
</script>
<!-- Zeta Digital - Large Item Shipping Calculator End -->