<!-- Zeta Digital - Apply In Stock Label, Count Down and Dispatch (+ Line 32-36, 53-57, 112) -->
{%- assign is_preorder_product = false -%}
{% for tag in product.tags %}
  {% if tag contains 'preorder' %}
      {%- assign is_preorder_product = true -%}
  {% endif %}
{% endfor %}
<!-- Zeta Digital - Apply In Stock Label, Count Down and Dispatch (+ Line 32-36, 53-57, 112) -->
{%- if template_layout == '5' -%}
    {%- assign centered = true -%}
{%- else -%}
    {%- assign centered = false -%}
{%- endif -%}
{%- if is_product_page -%}
    {%- assign change_history = true -%}
{%- endif -%}
{%- if product.metafields.sizeguide.html == 'hide' -%}
    {%- assign show_size_guide = false -%}
{%- endif -%}
{% capture cart_icon_name %}{{ settings.cart_icon | default: 'icon-theme-109' }}{% endcapture %}
{% capture description_html %}
    {%- if metafields.description.content != blank -%}
        <div class="product-page-info__description mb-30 text-center{% unless template_layout == '5' %} text-lg-left{% endunless %}">
            <div class="rte">
                {{ metafields.description.content }}
            </div>
        </div>
    {%- elsif description_place == 'info' and description != blank -%}
        <div class="product-page-info__description mb-30 text-center{% unless template_layout == '5' %} text-lg-left{% endunless %}">
            <div class="rte">
                {{ description }}
            </div>
        </div>
    {%- endif -%}
{% endcapture %}
{% capture countdown_html %}
    {%- if show_countdown -%}
        <div class="product-page-info__countdown mb-10">
            {% capture countdown_title %}{{ 'products.product.countdown.title' | t }}{% endcapture %}
            {% render 'product-get-countdown' with countdown_date: countdown_date, countdown_type: 2, countdown_title: countdown_title, centered: centered %}
        </div>
    {%- endif -%}
{% endcapture %}
{% capture stock_countdown_html %}
    {%- if show_stock_countdown -%}
        <div class="product-page-info__stock-countdown mb-20">
            <div class="stock-countdown{% unless quantity > 0 and quantity <= stock_countdown_min %} d-none-important{% endunless %}" data-js-product-stock-countdown data-min="{{ stock_countdown_min }}">
                {% capture quantity_html %}<span class="stock-countdown__counter">{{ quantity }}</span>{% endcapture %}
                <p class="stock-countdown__title h5 ls-0 mb-5 text-center{% if template_layout != '5' %} text-lg-left{% endif %}" data-js-product-stock-countdown-title>{{ 'products.product.stock_countdown_html' | t: quantity: quantity_html }}</p>
                {%- if show_stock_countdown_range -%}
                    <div class="stock-countdown__range">
                        {%- assign countdown_range_quotient = stock_countdown_min | times: 1.0 | divided_by: 100 -%}
                        {%- assign countdown_range_procent = quantity | divided_by: countdown_range_quotient -%}
                        <div class="stock-countdown__progress" data-js-product-stock-countdown-progress style="width: {{ countdown_range_procent }}%;"></div>
                    </div>
                {%- endif -%}
            </div>
        </div>
    {%- endif -%}
{% endcapture %}
{% capture text_countdown_html %}
    {%- if show_text_countdown -%}
		<!-- Zeta Digital - Custom Delivery Information Start -->
        <div class="hide-test_54">
            <div class="product-page-info__text-countdown">
                <div class="text-countdown">
                  {% if is_preorder_product %}
                    <h5><i class="mr-5">{% render 'icon-theme-127' %}</i>NEXT DAY DISPATCH WHEN ITEM ARRIVES</h5>
                  {% else %}
                  	<h5><i class="mr-5">{% render 'icon-theme-127' %}</i>NEXT BUSINESS DAY DISPATCH</h5>
                  {% endif %}
                  <p>Order today and your item will be dispatched on the next business day </p>
                </div>
            </div>
        </div>
		<!-- Zeta Digital - Custom Delivery Information End -->
    {%- endif -%}
{% endcapture %}
{% capture visitors_html %}
    {%- if show_visitors -%}
        <div class="product-page-info__visitors mt-25 mb-25">
            <div class="visitors ls-0 text-center{% if template_layout != '5' %} text-lg-left{% endif %} js-visitors" data-min="{{ settings.product_visitors_min }}" data-max="{{ settings.product_visitors_max }}" data-interval-min="{{ settings.product_visitors_interval_min }}" data-interval-max="{{ settings.product_visitors_interval_max }}" data-stroke="{{ settings.product_visitors_stroke }}">
                {%- assign visitors_random_diff = settings.product_visitors_max | minus: settings.product_visitors_min -%}
                {%- assign visitors_random = "now" | date: "%N" | modulo: visitors_random_diff | plus: settings.product_visitors_min -%}
                {% capture visitors_counter_html %}<span class="visitors__counter d-inline-block px-8" data-js-counter>{{ visitors_random }}</span>{% endcapture %}
                {{ 'products.product.visitors_html' | t: counter: visitors_counter_html }}
            </div>
        </div>
    {%- endif -%}
{% endcapture %}
{% capture free_shipping_html %}
    {%- if show_free_shipping and settings.cart_show_free_shipping -%}
        <div class="product-page-info__free-shipping mt-10 mb-15">
            {% render 'free-shipping' with mobile_centered: true centered: centered %}
        </div>
    {%- endif -%}
{% endcapture %}
{% capture payments_html %}
    {%- if show_payments -%}
        <div class="product-page-info__payments product-page-info__field d-flex flex-column align-items-start{% if template_layout == '5' %} align-items-lg-center{% endif %} mt-20 mb-10">
            <label class="mb-6">{{ 'products.product.payments' | t }}:</label>
            {% include 'payments' with payments_sequence: settings.payment_sequence_product %}
        </div>
    {%- endif -%}
{% endcapture %}
{%- if show_reviews -%}
    {% capture reviews_html %}
        {%- if settings.reviews_type == 'default' -%}
            {% render 'product-get-review' with id: id hide_empty: hide_empty_reviews hide_counter: hide_reviews_counter hide_mobile_counter: hide_reviews_mobile_counter %}
        {%- elsif settings.reviews_type == 'growave' and settings.app_growave_enable == true -%}
            {% capture the_snippet_review_avg %}{% render 'ssw-widget-avg-rate-profile' %}{% endcapture %}
            {%- if hide_empty_reviews and product.metafields.ssw['review'].avg == 0 -%}
                {%- assign hide_growave_review = true -%}
            {%- endif -%}
            {%- unless the_snippet_review_avg contains 'Liquid error' or hide_growave_review -%}
                <div class="mb-5">
                    {{ the_snippet_review_avg }}
                </div>
            {%- endunless -%}
        {%- endif -%}
    {% endcapture %}
{%- endif -%}
