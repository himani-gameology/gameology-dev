//MAIN MIXIN
@function color($color, $default, $default2: false) {
    @if $color == false or lightness($color) == 0 {
        @if $color != $default and $color != #000000 {
            $color: $default;
        } @else if $default2 != false {
            $color: $default2;
        }
    }

    @return $color;
}

