//presentation
{% if settings.presentation_enable %}
.presentation {
  float: right;
  width: 180px;
  z-index: 99999;
  transform: translate3d(100%, 0, 0);
  @include shadow(0 0 20px 0 rgba(0, 0, 0, 0.15));
  transition: $animation-css-duration transform, $animation-css-duration box-shadow;
  will-change: transform, box-shadow;
  &.open {
    transform: none !important;
  }
}
.presentation__btn {
  width: 30px;
  height: 30px;
  i {
    position: absolute;
    transition: $animation-css-duration transform;
    will-change: transform;
    &:first-child {
      transform: scale3d(1, 1, 1);
      .icon {
        width: 24px;
      }
    }
    &:last-child {
      transform: scale3d(0, 0, 1);
      .icon {
        width: 16px;
      }
    }
  }
}
.presentation.open .presentation__btn {
  width: 30px;
  height: 30px;
  i {
    &:first-child {
      transform: scale3d(0, 0, 1);
    }
    &:last-child {
      transform: scale3d(1, 1, 1);
    }
  }
}
.presentation__content {
  label {
    p {
      transition: $animation-css-duration color;
    }
  }
  a {
    transition: $animation-css-duration color;
  }
  .input-boolean input ~ span,
  .input-boolean input + span span {
    transition-duration: $animation-css-duration;
  }
}
[dir="rtl"] {
  .presentation {
    float: left;
    transform: translate3d(-100%, 0, 0);
    &.close {
      transform: translate3d(-100%, 0, 0) !important;
    }
  }
}
{% endif %}

