.breadcrumb {
    justify-content: space-between;
    align-items: center;
    position: relative;
    margin-top: 20px;
    @media (max-width: 999px) {
        margin-top: 12px;
        padding-bottom: 25px;
    }
    &:after {
        content: "";
        position: absolute;
        width: calc(100% - 30px);
        left: 0;
        top: 16px;
        height: 1px;
        background-color: rgba(0, 0, 0, 0.3);
    }
}
.breadcrumb__item {
    position: relative;
    min-width: 33px;
    text-align: center;
    &:before {
        width: 31px;
        height: 31px;
        border: 1px solid rgba(0, 0, 0, 0.3);
        text-align: center;
        font-size: 16px;
        border-radius: 100%;
        line-height: 31px;
        position: absolute;
        top: 0;
        background-color: #fff;
        z-index: 1;
        left: calc(50% - 16px);
        color: rgba(0, 0, 0, 0.3);
        pointer-events: none;
    }
    &:nth-child(1):before {
        content: "1";
    }
    &:nth-child(2):before {
        content: "2";
    }
    &:nth-child(3):before {
        content: "3";
    }
    &:nth-child(4):before {
        content: "4";
    }
    &.breadcrumb__item--completed {
        .breadcrumb__link, .breadcrumb__text {
            color: rgba(0, 0, 0, 0.3);
        }
        &:before {
            content: url({{ 'icon-check.svg' | asset_url }});
        }
    }
    &.breadcrumb__item--current {
        .breadcrumb__link, .breadcrumb__text {
            color: #FF414D;
        }
        &:before {
            border-color: #FF414D;
            color: #FF414D;
        }
    }
}
.breadcrumb__chevron-icon {
    display: none;
}
.breadcrumb__link,
.breadcrumb__text {
    width: 100%;
    padding-top: 40px;
    text-transform: uppercase;
    font-size: 8px;
