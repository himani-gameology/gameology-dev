  left: 50%;
  margin-left: -186px;
  opacity: 0;
  transition: opacity .3s;
  box-shadow: 1px 1px 10px 1px lightgrey;
}
#tooltip #tooltip-text::after {
  content: "";
  position: absolute;
  top: 100%;
  left: 50%;
  margin-left: -11px;
  border-width: 12px;
  border-style: solid;
  border-color: white transparent transparent transparent;
}
#tooltip:hover #tooltip-text {
  visibility: visible;
  opacity: 1;
