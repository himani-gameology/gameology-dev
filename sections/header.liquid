<script>
  document.addEventListener('DOMContentLoaded', function() {
  var searchForm = document.querySelector('form');
  var searchInput = document.querySelector('input[name="q"]');
  searchForm.addEventListener('submit', function(event) {
    event.preventDefault();
    var searchQuery = searchInput.value.trim();
    var selectedCategories = [];
    document.querySelectorAll('#category-options-mobile input[type="checkbox"]:checked').forEach(function(checkbox) {
      if (checkbox.value !== 'all') {
        selectedCategories.push(checkbox.value);
      }
    });

    Searchspring.search({
      query: searchQuery,
      filters: {
        categories: selectedCategories
      }
    });
  });
});

</script>




                    
<script>
  document.addEventListener('DOMContentLoaded', function() {
    var categoryButton = document.getElementById('category-button');
    var categoryOptions = document.getElementById('category-options');
    var categoryButtonMobile = document.getElementById('category-button-mobile');
    var categoryOptionsMobile = document.getElementById('category-options-mobile');
  
    categoryButton.addEventListener('click', function() {
      categoryOptions.classList.toggle('d-none');
      categoryButton.classList.toggle('open');
    });
  
    document.addEventListener('click', function(event) {
      if (!categoryButton.contains(event.target) && !categoryOptions.contains(event.target)) {
        categoryOptions.classList.add('d-none');
        categoryButton.classList.remove('open');
      }
    });

    categoryButtonMobile.addEventListener('click', function(event) {
      event.stopPropagation();
      categoryOptionsMobile.classList.toggle('d-none');
      categoryButtonMobile.classList.toggle('open');
    });

    document.addEventListener('click', function(event) {
      if (!categoryButtonMobile.contains(event.target) && !categoryOptionsMobile.contains(event.target)) {
        categoryOptionsMobile.classList.add('d-none');
        categoryButtonMobile.classList.remove('open');
      }
    });
    
  });
</script>

