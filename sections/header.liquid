                        </div>
                      </div>
                    </div>
                  {%- endif -%}
                  <div
                    class="{% if section.settings.vertical_menu != blank %}col-lg-9{% else %}col-12{% endif %} position-lg-static"
                    data-js-megamenu-width
                  >
                    {{ nav_html }}
                  </div>
                </div>
                {{ btn_search_html }}
              </div>
              {{ sticky_sidebar_html }}
            </div>
          </div>
        </div>
    {%- endcase -%}
    {%- assign selected_blocks = section.blocks | where: 'type', 'bottom_tape' -%}
    {%- for block in selected_blocks -%}
      {%- assign show_bottom_tape = null -%}
      {%- assign for_page = block.settings.for_page | remove: '' | split: ',' -%}
      {%- if for_page contains template -%}
        {%- assign show_bottom_tape = true -%}
      {%- elsif for_page contains page.handle -%}
        {%- assign show_bottom_tape = true -%}
      {%- elsif 'index' contains for_page -%}
        {%- assign show_bottom_tape = true -%}
      {%- elsif for_page contains 'all' and template != 'index' -%}
        {%- assign show_bottom_tape = true -%}
      {%- endif -%}
      {%- if show_bottom_tape -%}
        <div class="header__bottom-tape position-relative d-none d-lg-flex justify-content-lg-center colorize-theme-bg">
          {%- if block.settings.image != blank -%}
            {% capture bottom_tape_image_size %}x{{ section.settings.bottom_tape_height }}{% endcapture %}
            <img
              src="{{ block.settings.image | img_url: bottom_tape_image_size }}"
              class="h-lg-100"
              alt="{{ block.settings.image.alt }}"
            >
          {%- endif -%}
        </div>
      {%- endif -%}
    {%- endfor -%}
  </header>
</div>

<script>
  {%- if section.settings.type == '5' and settings.search_ajax -%}
  Loader.requireh({type: "script", name: "search_ajax"});
  {%- endif -%}
  {%- if section.settings.sticky != 'disable' -%}
  Loader.requireh({type: "script", name: "sticky_header"});
  {%- endif -%}
  Loader.requireh({type: "script", name: "header"});
</script>

