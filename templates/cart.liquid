<!-- Zeta Digital - Limit Product Quantity  -->
{{ 'product_qty_limit.js' | asset_url | script_tag }}
<script>
  function updateProductQty(productId, el){
   var limitList = productQuantityLimit;
    console.log(limitList);
    productId = parseInt(productId);
    var totalQty = jQuery(el).attr('data-product-quantity') / 2;

    var cartCheckQty =  jQuery(el).val();
      if(productId in limitList && cartCheckQty > limitList[productId]){
        alert(theme.strings.general.popups.cart.limit_is_exceeded_per_customer.replace('x_item_limit', limitList[productId]));
        jQuery(el).val(limitList[productId]);
        return false;
    }
    if (totalQty){
      if (parseInt(el.value) > parseInt(totalQty)){
        el.value = parseInt(totalQty);
        alert('Product not added. Product cannot exceed maximum purchase quantity!');
      }
    }
  }
</script>

<!-- Zeta Digital - Update Shipping Estimator Tonight Shipping Name (24/06/2021)  -->
<script>
  setTimeout(function(){
    updateToniteShippingName();
    jQuery('.cbb-shipping-rates-calculator-update-button').on('click', function(){
      setTimeout(function(){ updateToniteShippingName(); }, 5000);
    });
  }, 4000);

  function updateToniteShippingName()
  {
    jQuery('.cbb-shipping-rates-rate-name').each(function(i){
      if (jQuery(this)[0].innerHTML.includes('Delivery Tonight')){
        var currentDatetime = new Date();
        var currentHour = currentDatetime.getHours();
        var currentDayOfWeek = currentDatetime.getDay();
        var tomorrowOrNextBusinessDayDelivery = false;
        if (currentDayOfWeek == 0 || currentDayOfWeek == 6 || (currentDayOfWeek == 5 && currentHour >= 13)){
          jQuery(this)[0].innerHTML = 'Next Business Day Delivery (Select Postcodes only. No PO Box and Business Address)  from';
          tomorrowOrNextBusinessDayDelivery = true;
        }
        else if (currentHour >= 13){
          jQuery(this)[0].innerHTML = 'Tomorrow Delivery (Select Postcodes only. No PO Box and Business Address)  from';
          tomorrowOrNextBusinessDayDelivery = true;
        }
        if (!tomorrowOrNextBusinessDayDelivery){
          jQuery(this)[0].innerHTML = 'Same-Day Delivery (Select Postcodes only. No PO Box and Business Address) from';
        }
      }
      else if (jQuery(this)[0].innerHTML.includes('Standard shipping')){
      	jQuery(this)[0].innerHTML = 'Standard Delivery';
      }
      else if (jQuery(this)[0].innerHTML.includes('Express shipping')){
      	jQuery(this)[0].innerHTML = 'Express Delivery  from';
      }
      else if (jQuery(this)[0].innerHTML.includes('Clayton South')){
      	jQuery(this)[0].innerHTML = 'Click & Collect from Store in Clayton South VIC 3169';
      }
      else if (jQuery(this)[0].innerHTML.includes('Brunswick')){
      	jQuery(this)[0].innerHTML = 'Click & Collect from Store in Brunswick VIC 3056';
      }
    });
    jQuery(".cbb-shipping-rates-calculator-change-shipping-address a").css({'border': 'solid 2px black', 'border-radius': '10px', 'padding': '10px' });
    if (jQuery(window).width() < 767) {
      jQuery(".cbb-shipping-rates-calculator-map").css("display","none");
    }
  }
</script>