{% include 'breadcrumbs' %}
{%- if settings.product_format_pjpg -%}
    {%- assign image_format = 'pjpg' -%}
{%- else -%}
    {%- assign image_format = null -%}
{%- endif -%}
{%- if cart.item_count > 0 -%}
{%- assign giftCardInCart = false -%}
<div class="container">
    <div class="cart-header d-flex align-items-center mt-20">
        <h1 class="h3 m-0">{{ 'cart.general.title' | t }}</h1>

        <div class="cart-countdown-container text-center" data-timer="{{ settings.cart_countdown }}">
            {% render 'icon-countdown' %}
            <span>{{ 'cart.general.countdown' | t }}
                <!--Conversionry - Add countdown at cart page, sync with flyout and checkout page-->
                <cart-countdown class="cart-countdown-timer" data-countdown="10"></cart-countdown>
            </span>
        </div>
    </div>
</div>

<div class="cart mb-60 mb-lg-70">
    <div class="container">
        {%- if cart.item_count > 0 -%}
            <form action="/cart" method="post" novalidate>
                <button type="submit" class="btn btn--text ml-auto cart-checkout-btn pl-0 pr-0 checkout-mobile" name="checkout">
                    {{ 'cart.general.checkout_securely' | t }}
                 </button>
                <div class="row">
                    <div class="col-lg-8 col-xl-9">
                        <div class="cart__items">
                            <div class="cart__head pb-10 border-bottom d-none d-sm-none d-md-none d-lg-block">
                                <div class="row">
                                    <div class="d-none d-sm-block col-sm-5 col-lg-4 col-xl-7"><label class="m-0">{{ 'cart.label.product' | t }}</label></div>
                                    <div class="col-sm-7 col-lg-8 col-xl-5">
                                        <div class="row">
                                            <div class="col-7 col-sm-4 col-lg-4 text-right text-lg-left"><label class="m-0">{{ 'cart.label.price' | t }}</label></div>
                                            <div class="col-3 col-sm-4 col-lg-4 text-center text-lg-left"><label class="m-0">{{ 'cart.label.quantity' | t }}</label></div>
                                            <div class="col-2 col-sm-4 col-lg-4 text-right"><label class="m-0">{{ 'cart.label.total' | t }}</label></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {%- for item in cart.items -%}
                                {% if item.gift_card == true  %}
                                    {%- assign giftCardInCart = true -%}
                                 {% endif %}
                                <div class="py-20 border-bottom">
                                    <div class="row d-flex align-items-center">
                                        <div class="col-8 col-sm-8 col-lg-4 col-xl-7 d-flex align-items-center mb-20 mb-sm-0">
                                            <a href="/cart/change?line={{ forloop.index }}&amp;quantity=0" class="mr-lg-20 d-none d-md-block d-lg-block">
                                                <i>{% render 'icon-theme-165' %}</i>
                                            </a>
                                            <div class="d-flex align-items-center flex-row w-100">
                                                {%- if item.image != blank -%}
                                                    <a href="{{ item.url | within: collections.all }}" class="cart__image mb-sm-15 mb-xl-0 mr-20 mr-sm-0 mr-xl-20">
                                                        {% render 'rimage' with image: item.image alt:item.image.alt  size: '150x' format: image_format attr: 'class="w-100"' %}
                                                    </a>
                                                {%- endif -%}
                                                <div class="d-flex flex-column cart-title">
                                                    <a href="{{ item.url }}">{{ item.product.title }}
                                                        <span class='booster-cart-item-success-notes' data-key='{{item.key}}'></span><span class='booster-cart-item-upsell-notes' data-key='{{item.key}}'></span>
                                                    </a>
													<!-- Zeta Digital - Preorder Release Date -->
                                                  	<p id="preorder-release-date-{{ item.id }}" style="color: red"></p>
                                                  	<script>
                                                      {% if item.product.description contains "Release Date:" %}
                                                          var description = `{{ item.product.description | escape }}`;
                                                          description = description.substr(description.search("Release Date:"), 100);
                                                          var datePattern = /[0-9]{2,}-*[a-zA-Z]{3,}-*[0-9]{2,}/i;
                                                          var monthPattern = /[a-zA-Z]{3,}-*[0-9]{2,}/i;
                                                          var quarterPattern = /[a-zA-Z0-9]{2,}-*[0-9]{2,}/i;
                                                          var releaseDate = description.match(datePattern);
                                                          if (releaseDate != null){
                                                            jQuery("#preorder-release-date-{{ item.id }}").html("Release Date: " + releaseDate);
                                                          }
                                                          else {
                                                            let releaseMonth = description.match(monthPattern);
                                                            if (releaseMonth != null){
                                                              jQuery("#preorder-release-date-{{ item.id }}").html("Release Date: " + releaseMonth);
                                                            }
                                                            else {
                                                              let releaseQuarter = description.match(quarterPattern);
                                                              if (releaseQuarter != null){
                                                                jQuery("#preorder-release-date-{{ item.id }}").html("Release Date: " + releaseQuarter);
                                                              }
                                                              else {
                                                                jQuery("#preorder-release-date-{{ item.id }}").css('display', 'none');
                                                              }
                                                            }
                                                          }
                                                      {% else %}
                                                          jQuery("#preorder-release-date-{{ item.id }}").css('display', 'none');
                                                      {% endif %}
                                                    </script>

                                                    {% render 'judgeme_widgets', widget_type: 'judgeme_preview_badge', jm_style: '', concierge_install: false, product: item.product %}

                                                    {%- unless item.product.has_only_default_variant -%}
                                                        <p class="mb-0">{{ item.variant.title }}</p>
                                                    {%- endunless -%}
                                                    {%- if settings.cart_show_vendor -%}
                                                        <p class="mt-10 mb-0">
                                                            <a href="/collections/{{ item.collections.first.handle }}?constraint=custom-filter+vendor={{ item.vendor | handleize }}">{{ item.vendor }}</a>
                                                        </p>
                                                    {%- endif -%}
                                                    {%- assign property_size = item.properties | size -%}
                                                    {%- if property_size > 0 -%}
                                                        {%- for p in item.properties -%}
                                                            {%- if p.first != '__shopify_send_gift_card_to_recipient' -%}
                                                          {%- if p.first != '__shopify_offset' -%}
                                                          {%- unless p.last == blank -%}
                                                            <p class="mt-10 mb-0">              
                                                                    <span>{{ p.first }}: </span>
                                                                    {%- if p.last contains '/uploads/' -%}
                                                                        <a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                                                                    {%- else -%}
                                                                        {{ p.last }}
                                                                    {%- endif -%}                                                               
                                                            </p>
                                                             {%- endunless -%}
                                                            {%- endif -%}
                                                          {%- endif -%}
                                                        {%- endfor -%}
                                                    {%- endif -%}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4 col-sm-4 col-lg-8 col-xl-5">
                                            <div class="row d-flex flex-column-reverse flex-md-row flex-lg-row align-items-center">
                                                <div class="col-4 col-sm-4 col-md-4 col-lg-4 text-right-mobile d-none d-sm-none d-md-block d-lg-block">
                                                    <div class="cart-item-price">
                                                      {% render 'collection-product-get-price' with price: item.price compare_at_price: item.variant.compare_at_price %}
                                                    </div>
                                                </div>
                                                <div class="col-8 col-sm-6 col-md-4 col-lg-4 p-0 text-right">
                                                    <input type="number" data-product-quantity='{{ item.product.variants[0].inventory_quantity }}' class="mb-0 text-center text-sm-left" onChange="updateProductQty('{{ item.product_id }}', this)" name="updates[]" id="updates_{{ item.key }}" value="{{ item.quantity }}" min="1">
                                                	<a href="/cart/change?line={{ forloop.index }}&amp;quantity=0" class="d-block d-sm-block d-md-none d-lg-none mt-5" style="color:#ff414d;text-decoration:underline;font-weight:100;">Remove</a>
                                              	</div>
                                                <div class="col-8 col-sm-6 col-md-4 col-lg-4 p-0 text-right mb-10 mb-sm-10 mb-md-0 mb-lg-0">
                                                  {% render 'collection-product-get-price' with price: item.line_price compare_at_price: null %}
                                                  {% if item.variant.compare_at_price > item.price %}
                                                  	<span class="d-block d-sm-block d-md-none d-lg-none" style="font-size:12px;">You Save {{ item.variant.compare_at_price | minus: item.price | times: item.quantity | money }}</span>
                                                  {% endif %}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            {%- endfor -%}
                        </div>
                        {%- if settings.cart_show_free_shipping -%}
                            <div class="pt-20">
                                {% render 'free-shipping' %}
                            </div>
                        {%- endif -%}
                        <div class="d-flex pt-10">
                            <a href="/collections/all" class="btn btn--text pl-0 pr-0">
                                <i class="mb-4 mr-4">{% render 'icon-theme-012' %}</i>{{ 'cart.general.continue_shopping' | t }}
                            </a>
                            <button type="submit" class="btn btn--text ml-auto pl-0 pr-0" name="update">
                                <i class="mb-4 mr-4">{% render 'icon-theme-010' %}</i>{{ 'cart.general.update' | t }}
                            </button>
                        </div>

                        <!--Conversionry - Restyle Cart reward 010722 -->
                        <div class="cart-bottom-row d-flex display-noneCardO">
    						<!-- Zeta Digital - Swell Rewards Integration (18/09/20) -->
                            {% if giftCardInCart == false %}
                            <div class="cart-rewards ml-auto{% if customer %} customer-login{% endif %}">
                                <div class="cart-rewards-col">
                                    <div class="cart-rewards-line line-1">
                                        <div class="cart-rewards-title-wrapper">
                                            <h3 class="h4 mb-5">GAMERS GUILD </h3>
                                            <p class="redeemP">Join to earn & redeem points</p>
                                        </div>
